create_nvariables <- function(nvar, n, mean, sd, sd_noise) {
x <- rnorm(n = n, mean = mean, sd = sd)
dat <- map(1:nvar, \(i) {
tibble(!!paste0("var_", i) := x + rnorm(n = n, mean = 0, sd = sd_noise))
}) |>
list_cbind()
dat
}
