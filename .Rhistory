library(tidyverse)
samp_10k <- tibble(values = rnorm(10000, 20, 10))
samp_10k %>%
summarize(mean_hat = mean(values),
sd_hat   = sd(values))
# Histogram
ggplot(samp_10k, aes(values)) +
geom_histogram(binwidth = 1)
# Density plot
ggplot(samp_10k, aes(values)) +
geom_density(bw = 1)
mu  <- mean(samp_10k$values)
sig <- sd(samp_10k$values)
samp_10k %>%
summarize(
prop_1sd = mean(values >= mu - sig  & values <= mu + sig),
prop_2sd = mean(values >= mu - 2*sig & values <= mu + 2*sig),
prop_3sd = mean(values >= mu - 3*sig & values <= mu + 3*sig)
)
samp_10k %>%
summarize(p_ge_35 = mean(values >= 35))
samp_10k %>% summarize(p_more_extreme = mean(values <= 5 | values >= 35))
samp_10k %>%
summarize(
prop_1sd = mean(values >= mu - sig  & values <= mu + sig),
prop_2sd = mean(values >= mu - 2*sig & values <= mu + 2*sig),
prop_3sd = mean(values >= mu - 3*sig & values <= mu + 3*sig)
)
samp_10k %>%
summarize(p_ge_35 = mean(values >= 35))
samp_10k %>% summarize(p_more_extreme = mean(values <= 5 | values >= 35))
